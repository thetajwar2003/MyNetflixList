@page
@model Web.Pages.ShowsModel
<div class="row">
<h1 class="display-2">Shows</h1>
 <table class="table">
        <tr>
            <th>Show Name</th>
            <th>Show Tags</th>

        </tr>
        @foreach (string name in Model.Title)
        {
                    <tr><td>@name</td></tr>
        }
        @foreach (string tag in Model.Tags)
        {
                    <tr><td>@tag</td></tr>
        }
 </table>
</div>